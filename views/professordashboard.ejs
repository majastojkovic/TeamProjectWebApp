<p class="text-primary">All themes</p>
<form class="d-flex">
        <input class="form-control me-sm-2" type="text" placeholder="Search">
        <button class="btn btn-outline-secondary" type="submit">Search</button>
</form>


<table id="professorThemesTable" class="table table-hover">
  <thead>
    <% let headers = ['Title', 'Description', 'Expiry Date']; %>
    <% headers.forEach(headerText => { %>
        <th scope="col"><%= headerText %></th>
    <% }); %>
  </thead>
  <tbody>
      <% themes.forEach(function(theme) { %>
          <tr class="table-light">

            <td><a href="/professor/theme/<%= theme.title %>"><%= theme.title %></a></td>
            <td><%= theme.description %></td>
            <td><%= theme.expiryDate %></td>
        </tr>

    <% }); %>


  </tbody>
</table>

