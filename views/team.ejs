<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <div class="container-fluid">
    <div class="collapse navbar-collapse" id="navbarColor01">
      <ul class="navbar-nav me-auto">
        <li>
          <img src="https://www.elfak.ni.ac.rs/images/logo_elfak_en.png" alt="">
        </li>
      </ul>
      <ul class="navbar-nav ml-auto">
        <li class="nav-item">
          <a class="nav-link" href="/professor/professorHome">Home</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/users/logout">Logout</a>
        </li>
      </ul>
    </div>
  </div>
</nav>
<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body">
      <h1 class="text-center mb-3">Information about team</h1>
      <br>
      <form>
        <div class="info">
          <div class="name">
            <p class="lead mb-3"><strong>Name:</strong> <%= team.name %></p>
          </div>
          <div class="numberOfMembers">
            <p class="lead mb-3"><strong>Number of members:</strong> <%= team.numberOfMembers %></p>
          </div>
          <div class="members">
            <p class="lead mb-3"><strong>Members:</strong> </p>
            <ol>
              <% students.forEach(function(student) { %>
              <li class="lead mb-3" style="margin-left:30px"><%= student.email %></li>
              <% });%>
            </ol>

          </div>
        <%  if(team.chosenTheme != null){%>
            <div class="chosenTheme">
              <p class="lead mb-3"><strong>Chosen:</strong> <%= team.chosen %></p>
            </div>
          </div>
          <%}%>
        <br>
        <span>
        <%  if(user.role == "professor"){ %>
           <a style="margin:10px" href="/professor/professorProfile">Go Back</a>
         <% } else{ %>
          <a style="margin:10px" href="/student/studentProfile">Go Back</a>
        <%}%>
        </span>
      </form>
    </div>
  </div>
</div>
